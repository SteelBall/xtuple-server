{
  "private": true,
  "name": "xtuple-server-commercial",
  "version": "0.9.1",
  "description": "xTuple Server Commercial Package",
  "dependencies": {
    "glob": "^4.0.2",
    "lodash": "^2.4.1",
    "mkdirp": "^0.5.0",
    "npmlog": "^0.1.1",
    "requireindex": "^1.1.0",
    "rimraf": "^2.2.8",
    "xtuple-server-lib": "^0.9.61"
  },
  "bundledDependencies": [
    "glob",
    "lodash",
    "mkdirp",
    "npmlog",
    "requireindex",
    "rimraf",
    "xtuple-server-sys-cups",
    "xtuple-server-sys-paths",
    "xtuple-server-sys-policy",
    "xtuple-server-sys-service",
    "xtuple-server-sys-ssh",
    "xtuple-server-sys-webmin",
    "xtuple-server-pg-tuner",
    "xtuple-server-pg-snapshotmgr"
  ],
  "devDependencies": {
    "mocha": "^1.20.1",
    "xtuple-server": "^0.9.0"
  },
  "peerDependencies": {
    "xtuple-server": "^0.9.0"
  },
  "linkDependencies": {
    "xtuple-server-sys-cups": "tasks/sys/cups",
    "xtuple-server-sys-paths": "tasks/sys/paths",
    "xtuple-server-sys-policy": "tasks/sys/policy",
    "xtuple-server-sys-service": "tasks/sys/service",
    "xtuple-server-sys-ssh": "tasks/sys/ssh",
    "xtuple-server-sys-webmin": "tasks/sys/webmin",
    "xtuple-server-pg-tuner": "tasks/pg/tuner",
    "xtuple-server-pg-snapshotmgr": "tasks/pg/snapshotmgr"
  },
  "globalDependencies": {
    "xtuple-server": "^0.9.0",
    "tasks/sys/service": ""
  },
  "symlinkExports": {
    "plans/*": "/usr/local/lib/node_modules/xtuple-server/plans/",
    "node_modules/xtuple-server-sys-*": "/usr/local/lib/node_modules/xtuple-server/node_modules/",
    "node_modules/xtuple-server-pg-*": "/usr/local/lib/node_modules/xtuple-server/node_modules/"
  },
  "taskOverrides": {
    "sys-paths": "local-paths",
    "sys-policy": "local-policy"
  },
  "files": [
    "tasks/",
    "plans/",
    "spec/",
    "index.js",
    "link-core.js"
  ],
  "keywords": [
    "xtuple",
    "server",
    "erp",
    "business",
    "database",
    "platform",
    "database",
    "api",
    "crm",
    "inventory",
    "distribution",
    "manufacturing",
    "enterprise"
  ],
  "scripts": {
    "preinstall": "nex install -g && nex install",
    "postinstall": "node link-core.js",
    "test": "mocha node_modules/xtuple-server/spec/index.js"
  },
  "preferGlobal": true,
  "engines": {
    "node": "0.11.13"
  },
  "cpu": [
    "!ia32"
  ],
  "os": [
    "linux"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/xtuple/xtuple-server-commercial.git"
  },
  "author": "Travis Webb <travis@xtuple.com>",
  "bugs": {
    "url": "https://github.com/xtuple/xtuple-server-commercial/issues"
  }
}
